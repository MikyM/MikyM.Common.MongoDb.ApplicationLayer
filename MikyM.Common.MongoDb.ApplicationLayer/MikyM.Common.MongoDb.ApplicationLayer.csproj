<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>MikyM.Common.MongoDb.ApplicationLayer</PackageId>
    <Version>1.0.0</Version>
    <Authors>Krzysztof Kupisz</Authors>
    <PackageDescription>Application layer for DDD architecture utilizing MikyM.Common.MongoDb.DataAccessLayer package.</PackageDescription>
    <RepositoryUrl>https://github.com/MikyM/MikyM.Common.MongoDb.ApplicationLayer</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MikyM.Common.ApplicationLayer" Version="1.2.6" />
    <PackageReference Include="MikyM.Common.MongoDb.DataAccessLayer" Version="1.1.0" />
  </ItemGroup>

</Project>
